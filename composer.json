{
    "name": "clarity-project/images-bundle",
    "description": "Symfony2 bundle for simplifying upload and cropping images",
    "keywords": ["images","symfony", "crop", "upload"],
    "license": "MIT",
    "authors": [
        {
            "name": "Zmicier Aliakseyeu",
            "email": "z.aliakseyeu@gmail.com"
        }
    ],
     
    "require": {
        "symfony/framework-bundle": "~2.2",
        "imagine/imagine": "dev-develop",
        "jfeldstein/jquery-ajax-upload": "dev-master"
    },

    "repositories": [
        {
            "type": "package",
            "version": "dev-master",
            "package": {
                "name": "jfeldstein/jquery-ajax-upload",
                "source": {
                    "url": "https://github.com/jfeldstein/jQuery.AjaxFileUpload.js.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        }
    ],

    "autoload": {
        "psr-0": {
            "Clarity\\ImagesBundle": ""
        }
    },

    "target-dir": "Clarity/ImagesBundle"
}